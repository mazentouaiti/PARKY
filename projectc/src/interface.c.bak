  GtkWidget *fixed20;
  GtkWidget *image20;
  GtkWidget *adminbtn;
  GtkWidget *clientbtn;
  GtkWidget *label77;

  welcome = gtk_window_new (GTK_WINDOW_TOPLEVEL);
  gtk_window_set_title (GTK_WINDOW (welcome), _("PARKY"));
  welcome_icon_pixbuf = create_pixbuf ("top-view-electric-cars-parking-lot.jpg");
  if (welcome_icon_pixbuf)
    {
      gtk_window_set_icon (GTK_WINDOW (welcome), welcome_icon_pixbuf);
      gdk_pixbuf_unref (welcome_icon_pixbuf);
    }

  fixed20 = gtk_fixed_new ();
  gtk_widget_show (fixed20);
  gtk_container_add (GTK_CONTAINER (welcome), fixed20);
  gtk_widget_set_size_request (fixed20, 423, 232);

  image20 = create_pixmap (welcome, "top-view-electric-cars-parking-lot.jpg");
  gtk_widget_show (image20);
  gtk_fixed_put (GTK_FIXED (fixed20), image20, 0, 0);
  gtk_widget_set_size_request (image20, 424, 256);

  adminbtn = gtk_button_new_with_mnemonic (_("Admin"));
  gtk_widget_show (adminbtn);
  gtk_fixed_put (GTK_FIXED (fixed20), adminbtn, 40, 188);
  gtk_widget_set_size_request (adminbtn, 100, 27);

  clientbtn = gtk_button_new_with_mnemonic (_("Client"));
  gtk_widget_show (clientbtn);
  gtk_fixed_put (GTK_FIXED (fixed20), clientbtn, 296, 199);
  gtk_widget_set_size_request (clientbtn, 100, 25);

  label77 = gtk_label_new (_("Log In As :"));
  gtk_widget_show (label77);
  gtk_fixed_put (GTK_FIXED (fixed20), label77, 160, 32);
  gtk_widget_set_size_request (label77, 104, 32);
  gtk_label_set_justify (GTK_LABEL (label77), GTK_JUSTIFY_FILL);
