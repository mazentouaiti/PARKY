  GLADE_HOOKUP_OBJECT (welcome, adminbtn, "adminbtn");
  GLADE_HOOKUP_OBJECT (welcome, clientbtn, "clientbtn");
  GLADE_HOOKUP_OBJECT (welcome, label77, "label77");

  return welcome;
}

