                    G_CALLBACK (on_adminbtn_clicked),
                    NULL);
  g_signal_connect ((gpointer) clientbtn, "clicked",
                    G_CALLBACK (on_clientbtn_clicked),
                    NULL);

  /* Store pointers to all widgets, for use by lookup_widget(). */
